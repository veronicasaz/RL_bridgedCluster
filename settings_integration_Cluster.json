{
    "InitialConditions":
    {
       "units": "si",
       "seed": 1,
       "n_bodies": 5, 
       "ranges_mass": [1, 100],
       "radius_cluster": 0.1,
 	"virial_ratio": 0.3,
	"disk_radius": [10, 50],
	"mass_disk": 0.02
    },
    "Integration":
    { 
        "savefile": "./Cluster_Testing_results/",
        "subfolder": "",
        "suffix": "",
	"savestate": true,
        "max_steps": 100,
        "check_step": 0.01, 
        "t_step_global": 1e-2,
        "t_step_local": 1e-1,
        "integrator_global": "Ph4",
        "integrator_local": "Huayno",
        "max_error_accepted": 1e-1,
        "plot": false
    },
    "RL":
    {
        "state": "potential",
        "state_options_info": ["norm", "cart", "dist", "potential"],
        "action": "range", 
        "number_actions": 11,
	"range_action": [1e-6, 1e-2],
	"reward_f": 1,
        "weights": [500.0, 10, 1.0]
     },

    "Training":
    {
    	"RemovePlanets": true,
    	"savemodel": "./Training_Results/",
    	"subfolder": "",
    	"suffix":"",
    	"display": true,
        "max_episodes": 1000,
        "testdata_size": 5,
        "hidden_layers": 5,
        "neurons": 200,
        "lr": 1e-3,
        "batch_size": 1000,
        "gamma": 0.99,
        "eps_start": 0.9,
        "eps_end": 0.05,
        "eps_decay": 1000,
        "tau": 0.005
    },
    "Hyperparam_optim":
    {
    "max_iter": 10,
    "individuals": 8,
    "bnds":{
    	    "epochs": [10, 1000],
            "layers": [2, 6],
            "neurons": [10, 125], 
            "lr": [1e-5, 1e-2]
              }
    
    }
}
